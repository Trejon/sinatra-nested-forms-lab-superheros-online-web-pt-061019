<h1>Create a Team and Heroes!</h1>
<form method="POST" action="/teams">
  <input type="text" name="team[name]" value="team_name">
  <input type="text" name="team[motto]" value="team_motto">
  
  <h2>Hero</h2>
  <input type="text" name="team[members][][name]" value="member1_name">
  <input type="text" name="team[members][][power]" value="member1_power">
  <input type="text" name="member1_bio" value="member1_bio">
  
</form>
